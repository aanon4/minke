#docker build -t registry.gitlab.com/minkebox/minke .
docker run --rm --name minke -d --privileged -v /etc/timezone:/etc/timezone:ro -v /var/run/docker.sock:/var/run/docker.sock --mount type=bind,source=/home/minke/fs,target=/minke/fs,bind-propagation=rshared --mount type=bind,source=/home/minke/db,target=/minke/db,bind-propagation=rshared --mount type=bind,source=${PWD}/app/skeletons/local,target=/app/skeletons/local,bind-propagation=rshared --network=host registry.gitlab.com/minkebox/minke
